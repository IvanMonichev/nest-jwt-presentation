MONGO_INITDB_DATABASE=nest-db

MONGO_HOST=localhost
MONGO_DB=nest-db
MONGO_PORT=27017
MONGO_USER=admin
MONGO_PASSWORD=password
MONGO_AUTH_BASE=admin

# Mongo Express UI auth
ME_CONFIG_BASICAUTH_USERNAME=admin
ME_CONFIG_BASICAUTH_PASSWORD=password
