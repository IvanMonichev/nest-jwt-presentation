JWT_ACCESS_TOKEN_SECRET=default-access-secret
JWT_REFRESH_TOKEN_SECRET=default-refresh-secret

# Сроки жизни токенов
JWT_ACCESS_TOKEN_EXPIRES_IN=900000        # 15 минут
JWT_REFRESH_TOKEN_EXPIRES_IN=604800000    # 7 дней

MONGO_INITDB_ROOT_USERNAME=admin
